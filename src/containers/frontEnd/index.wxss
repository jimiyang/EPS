#frontEnd {
  width: 100%;
}
#frontEnd #header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 42px;
  padding: 0 60px;
  border-bottom: 1px solid #797979;
  background: #f2f2f2;
}
#frontEnd #header ul li {
  color: #fc3339;
  cursor: pointer;
}
#frontEnd #header > div {
  display: flex;
  justify-content: center;
  align-items: center;
}
#frontEnd #header > div img {
  width: 30px;
  height: 30px;
  margin-right: 5px;
  border-radius: 50%;
}
#frontEnd #header > div span {
  margin: 0 3px;
}
#frontEnd #header > div div {
  display: flex;
  justify-content: center;
  align-items: center;
}
#frontEnd #header > div div .not {
  color: #fc3339;
}
#frontEnd #header .myOrder {
  color: #1DC06F;
}
#frontEnd #search {
  height: 78px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #d7d7d7;
  padding: 0 60px;
}
#frontEnd #search h1 {
  font-size: 20px;
}
#frontEnd #search .search {
  display: flex;
  justify-content: center;
  align-items: center;
  cursor: pointer;
}
#frontEnd #search .search input {
  width: 350px;
  height: 40px;
  border: 1px solid #ccc;
  padding: 0 10px;
}
#frontEnd #search .search input:focus {
  outline: none;
}
#frontEnd #search .search div {
  background: #aeaeae;
  width: 40px;
  height: 40px;
  display: flex;
  justify-content: center;
  align-items: center;
}
#frontEnd #search .search div img {
  width: 20px;
}
